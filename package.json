    "lint:tsc": "tsc --project tsconfig-ci-lint.json",
    "lint:prettier": "prettier --check .",
    "test": "LOG_LEVEL=${LOG_LEVEL:-silent} jest --detectOpenHandles --maxWorkers=30% ./src --passWithNoTests",
